@model BurialTextileData

<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">Burial Details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <dl class="row">
        <dt class="col-sm-4">ID</dt>
        <dd class="col-sm-8">@Model.BurialId</dd>
        <dt class="col-sm-4">Excavation Year</dt>
        <dd class="col-sm-8">@Model.ExcavationYear</dd>
        <dt class="col-sm-4">Area</dt>
        <dd class="col-sm-8">@Model.Area</dd>
        <dt class="col-sm-4">Sex</dt>
        <dd class="col-sm-8">@Model.Sex</dd>
        <!-- Add other fields as needed -->
    </dl>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
</div>
